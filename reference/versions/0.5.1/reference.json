{
    "version": "0.5.1",
    "analyses": {
        "buffer": {
            "params": {
                "source": {
                    "type": "node"
                },
                "radius": {
                    "type": "number"
                }
            }
        },
        "moran": {
            "params": {
                "source": {
                    "type": "node"
                },
                "numerator_column": {
                    "type": "string"
                },
                "denominator_column": {
                    "type": "string"
                },
                "significance": {
                    "type": "number"
                },
                "neighbours": {
                    "type": "number"
                },
                "permutations": {
                    "type": "number"
                },
                "w_type": {
                    "type": "enum",
                    "values": [
                        "knn",
                        "queen"
                    ]
                }
            }
        },
        "intersection": {
            "params": {
                "source_a": {
                    "type": "node"
                },
                "source_b": {
                    "type": "node"
                }
            }
        },
        "aggregate-intersection": {
            "params": {
                "source_a": {
                    "type": "node"
                },
                "source_b": {
                    "type": "node"
                },
                "aggregate_function": {
                    "type": "enum",
                    "values": [
                        "avg",
                        "count",
                        "max",
                        "min",
                        "sum"
                    ]
                },
                "aggregate_column": {
                    "type": "string"
                }
            }
        },
        "population-in-area": {
            "params": {
                "source": {
                    "type": "node"
                },
                "final_column": {
                    "type": "string"
                }
            }
        },
        "source": {
            "params": {
                "query": {
                    "type": "string"
                }
            }
        },
        "trade-area": {
            "params": {
                "source": {
                    "type": "node"
                },
                "kind": {
                    "type": "enum",
                    "values": [
                        "walk",
                        "drive",
                        "bike"
                    ]
                },
                "time": {
                    "type": "number"
                }
            }
        }
    }
}